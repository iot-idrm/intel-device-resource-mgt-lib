<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping> 
    <class name="com.intel.iot.ams.entity.AmsTask" table="AmsTask"> 
        <id name="id" type="java.lang.Integer"> 
            <column name="id" /> 
            <generator class="identity" /> 
        </id> 
        <property name="taskType" type="int"> 
            <column name="taskType" /> 
        </property> 
        <property name="taskProperties" type="string"> 
            <column name="taskProperties" /> 
        </property> 
        <property name="taskPriority" type="int"> 
            <column name="taskPriority" /> 
        </property> 
        <property name="taskCreateTime" type="java.util.Date"> 
            <column name="taskCreateTime" /> 
        </property> 
        <property name="taskExpiry" type="int"> 
            <column name="taskExpiry" /> 
        </property> 
    </class> 
</hibernate-mapping>