<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping> 
    <class name="com.intel.iot.ams.entity.Product" table="Product"> 
        <id name="id" type="java.lang.Integer"> 
            <column name="id" /> 
            <generator class="identity" /> 
        </id> 
        <property name="uuid" type="string"> 
            <column name="uuid" /> 
        </property> 
        <property name="name" type="string"> 
            <column name="name" /> 
        </property> 
        <property name="category" type="java.lang.Integer"> 
            <column name="category" /> 
        </property> 
        <property name="description" type="string"> 
            <column name="description" /> 
        </property> 
        <property name="vendor" type="string"> 
            <column name="vendor" /> 
        </property> 
        <property name="defaultVersion" type="string"> 
            <column name="defaultVersion" /> 
        </property> 
    </class> 
</hibernate-mapping>