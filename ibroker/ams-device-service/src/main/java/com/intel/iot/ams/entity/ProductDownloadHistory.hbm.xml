<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping> 
    <class name="com.intel.iot.ams.entity.ProductDownloadHistory" table="ProductDownloadHistory"> 
        <id name="id" type="java.lang.Integer"> 
            <column name="id" /> 
            <generator class="identity" /> 
        </id> 
        <property name="amsClientUuid" type="string"> 
            <column name="amsClientUuid" /> 
        </property> 
        <property name="productName" type="string"> 
            <column name="productName" /> 
        </property> 
        <property name="category" type="java.lang.Integer"> 
            <column name="category" /> 
        </property> 
        <property name="fromId" type="java.lang.Integer"> 
            <column name="fromId" /> 
        </property> 
        <property name="toId" type="java.lang.Integer"> 
            <column name="toId" /> 
        </property> 
        <property name="downloadTime" type="java.util.Date"> 
            <column name="downloadTime" /> 
        </property> 
        <property name="isAot" type="java.lang.Boolean"> 
            <column name="isAot" /> 
        </property> 
    </class> 
</hibernate-mapping>